<% @pagescontents.each do |pagescontent| %>
<div class="parallax-container valign-wrapper" id="parallax-container">
	<div class="section no-pad-bot">
		<div class="container">
			<div class="row center">
				<h3 class="header col s12 light"><strong><%= pagescontent.name %></strong></h3>
			</div>
		</div>
	</div>
	<div class="parallax">
		<%= image_tag pagescontent.image, id: 'parallax-img' %>
	</div>
</div>
<% end %>